.main_content {
  font-size: 0px;
  background: #EEEEEE;
}

.nav_cell {
  z-index: 2;
  position: fixed;
  text-align: center;
  top:0px;
  left: 0px;
  background: #3891F8;
  width: 100vw;
  height: 50px;
  line-height: 50px;
  
}


.nav {
  display: inline-block;
  width: 33.3%;
}

.nav {
  color: #fff;
  font-size: 15px;
}
 .nav text {
  font-weight: bold;
  display: inline-block;
  line-height: 26px;
}

.decoration {
  border-bottom: 2px solid #fff;
}
.all_order{
  margin-top: 50px;
  overflow: scroll;
  z-index: 2;
}
.each_order_cell{
  background: #fff;
  margin-bottom: 10px;
}
.store_name_state{
  border-bottom: 1rpx solid #EEEEEE;
}
.store_content{
  margin: 0px 15px;
  font-size: 12px;
  text-align: left;
  line-height: 32px;
}
.store_content image{
  width:14px;
  height: 13px;
  vertical-align: top;
  margin-top: 10px;
  margin-right: 8px;
}
.store_name{
  color:#B3B3B3;
}
.order_state{
  color:#3891F8;
  float:right;
}
.service_infor_content{
  margin: 0px 15px;
  padding-top: 10px;
  text-align: left;
}
.service_pic{
  display: inline-block;
  width:21%;
}
.service_infor_content image{
  width: 60px;
  height: 60px;
}
.service_name_money{
  height: 60px;
  width: 75%;
  margin-left: 4%;
  display: inline-block;
  vertical-align: top;
  border-bottom: 1rpx solid #EEEEEE;
}
.service_name{
  margin-top: 4px;
  font-size: 18px;
  color: #333333;
  font-weight: bold;
}
.service_money{
  margin-top: 8px;
  line-height: 15px;
}
.service_num{
  color:#B3B3B3;
  font-size: 11px;
   vertical-align: middle;
}
.service_price{
   color:#3891F8;
   vertical-align: middle;
  font-size: 15px;
  float: right;
}
.order_btn{
  text-align: right;
}
.each_btn{
  margin: 15px 0px;
  font-size:15px;
  text-align: center;
  margin-right: 15px;
  display: inline-block;
}
.btnBac{
}
.each_btn text{
  display: inline-block;
  margin: 7px 0px;
  width: 100px;
}
.each_btn .formIdBtn{
  display: inline-block;
  text-align: center;
  width:100px;
  color:#3891F8;
  border:1px solid #3891F8;
  background: none;
  border-radius: 200px;
  line-height: 34px;
  font-size: 15px;

}
.btnBac button{
  background: #3891F8#3891F8!important;
  color:#fff!important;
}