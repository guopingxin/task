<form bindsubmit='submitFormId' report-submit='true'>
  <button form-type='submit' hover-class="none" class='formIdBtn'>
    <view class='' wx:if='{{myClaims}}'>
      <view class='oprationBtm' wx:if='{{detail.status==3}}'>
        <button class='accept' bindtap='orders'>接 单</button>
      </view>
      <view class='oprationBtm' wx:if='{{detail.status==4||detail.status==6}}'>
        <button class='addProBtn' wx:if='{{!hasReach}}' bindtap='reachScene' style='width:30%;'>到达现场 </button>
        <button style='display:inline-block;width:30%;margin-left:30px;font-size:14px;' wx:if='{{detail.is_push==0&&hasPush&&detail.type==0}}' class='addProBtn overTask' bindtap='tofix'>推修</button>
        <button style='display:inline-block;width:30%;margin-left:30px;font-size:14px;' wx:if='{{detail.is_push==1&&hasPush&&detail.type==1&&pushDetail.result==0}}' class='addProBtn overTask' bindtap='openFeedback'>反馈推修</button>
        <button style='display:inline-block;width:30%;margin-left:30px;font-size:14px;' wx:if='{{detail.is_push==1&&hasPush&&detail.type==0}}' class='addProBtn overTask'>已推修</button>
      </view>
      <view class='oprationBtm' wx:if='{{allEnd}}'>
        <view style='display:inline-block;width:{{viewWidth}};vertical-align:top;height:60px' wx:if='{{hasReach}}'>
          <button style='display:inline-block;width:{{widthView}};font-size:14px;' wx:if='{{!inDetail}}' class='addProBtn overTask' bindtap='addPicBtn'>添加明细</button>

          <button style='background:#3791F6;display:inline-block;width:{{widthView}}' wx:if='{{inDetail}}' class='addProBtn overTask' bindtap='backDetail'>返回详情</button>
          <view style='display:inline-block;width:4%' wx:if='{{hasPush}}'></view>
          <button style='display:inline-block;width:48%;font-size:14px;' wx:if='{{detail.is_push==0&&hasPush&&detail.type==0}}' class='addProBtn overTask' bindtap='tofix'>推修</button>
          <button style='display:inline-block;width:48%;font-size:14px;' wx:if='{{detail.is_push==1&&hasPush&&detail.type==1&&pushDetail.result==0}}' class='addProBtn overTask' bindtap='openFeedback'>反馈推修</button>
          <button style='display:inline-block;width:48%;font-size:14px;' wx:if='{{detail.is_push==1&&hasPush&&detail.type==0}}' class='addProBtn overTask'>已推修</button>
        </view>
        <view style='display:inline-block;width:{{spacing}}'></view>
        <button wx:if='{{hasReach}}' style='width:{{btnView}};' class='addProBtn overTask' bindtap='finish'>结 　案</button>
      </view>
      <view class='oprationBtm' wx:if='{{endCase}}'>
        <button style='display:inline-block;width:40%' wx:if='{{!inDetailEnd}}' class='addProBtn overTask' bindtap='addPicBtn'>添加明细</button>
       
        <button style='display:inline-block;width:40%' wx:if='{{inDetailEnd}}' class='addProBtn overTask' bindtap='backDetail'>返回详情</button>
         <view style='width:10%;display:inline-block' wx:if='{{detail.is_push==1&&hasPush&&detail.type==1&&pushDetail.result==0}}'></view>
        <button style='display:inline-block;width:40%' wx:if='{{detail.is_push==1&&hasPush&&detail.type==1&&pushDetail.result==0}}' class='addProBtn overTask' bindtap='openFeedback'>反馈推修</button>
         <view style='width:10%;display:inline-block' wx:if='{{detail.type==0}}'></view>
     
        <button wx:if='{{detail.is_push==0&&hasPush&&detail.type==0}}' style='display:inline-block;width:40%' class='addProBtn overTask' bindtap='tofix'>推 　修</button>
        <button wx:if='{{detail.is_push==1&&hasPush&&detail.type==0}}' style='display:inline-block;width:40%' class='addProBtn overTask'>已推修</button>
      </view>
    </view>
  </button>
</form>

<view class='modalDe' wx:if='{{modal}}' catchtap='closeOprationModal'>
  <view class='modalContentDe' style='height:{{ss}}' catchtap='stopBubble'>

    <view class='eachInformodalDe' bindtap='toEdit' wx:if="{{detail.status != 6}}">
      <text class='zan-icon zan-icon-passed'></text>　编辑</view> 
      <!-- zan-icon-edit -->

    <view wx:if='{{detail.status==0}}' class='eachInformodalDe' catchtap='checkModal'>
      <text class='zan-icon zan-icon-passed'></text>　审核</view>

    <view class='eachInformodalDe' wx:if='{{detail.status==2||detail.status==3||detail.status==5||detail.status==4}}' catchtap='toSelectPeople'>
      <text class='zan-icon zan-icon-exchange-record'></text>　分配</view>

    <view class='eachInformodalDe' catchtap='toDelete'>
      <text class='zan-icon zan-icon-close'></text>　删除</view>

      <!-- wx:if='{{detail.task_id}}' -->
    <view class='eachInformodalDe' wx:if='{{detail.status != 6}}' bindtap='cancelClaim' >
      <text class='zan-icon zan-icon-edit-data'></text>　取消</view>
  </view>
</view>

