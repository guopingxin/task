<!--pages/index/claimsDetail/claimsDetail.wxml-->
<import src="../../../template/step1/step1.wxml" />
<import src="../../../template/step/step.wxml" />
<import src="../../template/getDataList/getDataList.wxml" />
<import src="../../template/top1.wxml" />
<import src="../../../template/top1.wxml" />
<template is="top" data="{{titleTop,back_cell,title_cell}}"></template>
<view class='{{container}}'>
<view class='containerDe'>
  <view wx:if='{{!addImage}}'>
    <view class='headerDe'>
      <view class='titDe'>业务基本信息
        <!-- <view wx:if='{{detail.status!=6&&opratorRank==1}}' bindtap='openModal' class='zan-icon zan-icon-wap-nav'></view> -->
        <view bindtap='openModal' class='zan-icon zan-icon-wap-nav'></view>
      </view>
    </view>
    <view class='sectionDe'>
      <view class='contentDe' bindtouchstart='enablePage'>
        <view class='eachInforDe'>
          <label class='inforLabel'>业务编号</label>
          <text class='inforTest'>{{detail.survey_no}}</text>
        </view>
        <view class='eachInforDe'>
          <label class='inforLabel'>查勘员</label>
          <text class='inforTest'>{{userName}}</text>
        </view>

        <view class='eachInforDe'>
          <label class='inforLabel'>工号</label>
          <text class='inforTest'>{{usernumber}}</text>
        </view>

        <view class='eachInforDe'>
          <label class='inforLabel'>报案号</label>
          <text class='inforTest'>{{detail.report_no}}</text>
        </view>
        <view class='eachInforDe'>
          <label class='inforLabel'>任务流类型</label>
          <text class='inforTest' wx:if='{{detail.type==0}}'>查勘</text>
          <text class='inforTest' wx:if='{{detail.type==1}}'>三责</text>
          <text class='inforTest' wx:if='{{detail.type==2}}'>标的</text>
        </view>
        

        <!-- <view class='eachInforDe'>
          <label class='inforLabel'>被保人</label>
          <text class='inforTest'>{{detail.recognizee}}</text>
        </view>
        <view class='eachInforDe' wx:if='{{detail.service_id}}' data-num='{{detail.mobile}}' bindtap='link'>
          <label class='inforLabel'>联系电话</label>
          <text class='phoneText zan-icon zan-icon-phone'></text>
          <text class='inforTest phoneText'> {{detail.mobile}}</text>
        </view> -->
        <!-- <view class='eachInforDe' wx:if='{{detail.car_no}}'>
          <label class='inforLabel'>车牌号码</label>
          <text class='inforTest'>{{detail.car_no}}</text>
        </view> -->

        <view class='eachInforDe' wx:if="{{detail.car_no}}">
          <label class='inforLabel'>车牌号码</label>
          <text class='inforTest'>{{detail.car_no}}</text>
        </view>

        <view class='eachInforDe'>
          <label class='inforLabel'>保险公司</label>
          <text class='inforTest'>{{insuranceCom.name}}</text>
        </view>

        <view class='eachInforDe'>
          <label class='inforLabel'>任务类型</label>
          <text class='inforTest'>{{detail.task_type}}</text>
        </view>

        <view class='eachInforDe' wx:if="{{detail.case_type}}">
          <label class='inforLabel'>案件类型</label>
          <text class='inforTest'>{{detail.case_type}}</text>
        </view>

        <view class='eachInforDe' wx:if="{{detail.send_time}}">
          <label class='inforLabel'>派工时间</label>
          <text class='inforTest'>{{detail.send_time}}</text>
        </view>

        <view class='eachInforDe' wx:if="{{detail.are}}">
          <label class='inforLabel'>区域</label>
          <text class='inforTest'>{{detail.are}}</text>
        </view>

        <view class='eachInforDe' wx:if="{{detail.send_date}}">
          <label class='inforLabel'>派工时间段</label>
          <text class='inforTest'>{{detail.send_date}}</text>
        </view>

        <view class='eachInforDe' wx:if="{{detail.report_date}}">
          <label class='inforLabel'>报案时间</label>
          <text class='inforTest'>{{detail.report_date}}</text>
        </view>

        <view class='eachInforDe' wx:if="{{detail.send_user}}">
          <label class='inforLabel'>派工人</label>
          <text class='inforTest'>{{detail.send_user}}</text>
        </view>

        <view class='eachInforDe' wx:if="{{detail.policy_no}}">
          <label class='inforLabel'>保单号</label>
          <text class='inforTest'>{{detail.policy_no}}</text>
        </view>

        <view class='eachInforDe' wx:if="{{detail.ins_org}}">
          <label class='inforLabel'>承保机构</label>
          <text class='inforTest'>{{detail.ins_org}}</text>
        </view>

        <view class='eachInforDe' wx:if="{{detail.survey_address}}">
          <label class='inforLabel'>出险/定损地点</label>
          <!-- <text class='inforTest'>{{detail.survey_address}}</text> -->
          <input value='{{detail.survey_address}}' style='width:70%;display:inline-block;vertical-align: middle'/>
        </view>

        <!-- <view class='eachInforDe' wx:if='{{detail.service_id}}'>
          <label class='inforLabel'>服务商</label>
          <text class='inforTest'>{{serviceInfor.short_name}}</text>
        </view> -->

        <view class='eachInforDe' wx:if='{{detail.task_id&&detail.task_id!=userId&&opratorRank==1}}'>
          <label class='inforLabel'>作业员</label>
          <text class='inforTest'>{{oprator.nickname}}</text>
        </view>
        <!-- <view class='eachInforDe' wx:if='{{detail.task_id&&detail.task_id!=userId&&opratorRank==1}}' data-num='{{oprator.mobile}}' bindtap='link'>
          <label class='inforLabel'>联系电话</label>
          <text class='phoneText zan-icon zan-icon-phone'></text>
          <text class='inforTest phoneText'> {{oprator.mobile}}</text>
        </view> -->
        <!-- <view class='eachInforDe'>
          <label class='inforLabel'>预估金额</label>
          <text class='inforTest'>{{detail.money}}</text>
        </view> -->
        <view class='eachInforDe'>
          <label class='inforLabel'>状态</label>
          <text class='inforTest' wx:if='{{detail.status==0}}'>待审核</text>
          <text class='inforTest' wx:if='{{detail.status==1}}'>审核失败</text>
          <text class='inforTest' wx:if='{{detail.status==2}}'>审核成功</text>
          <text class='inforTest' wx:if='{{detail.status==3}}'>已分配</text>
          <text class='inforTest' wx:if='{{detail.status==4}}'>进行中</text>
          <text class='inforTest' wx:if='{{detail.status==5}}'>已取消</text>
          <text class='inforTest' wx:if='{{detail.status==6}}'>已完成</text>
        </view>
        <view class='eachInforDe' wx:if="{{detail.survey_date}}">
          <label class='inforLabel'>查勘时间</label>
          <text class='inforTest'>{{detail.survey_date}}</text>
        </view>
        <view class='eachInforDe' wx:if='{{detail.update_time}}'>
          <label class='inforLabel'>最后更新</label>
          <text class='inforTest'>{{detail.update_time}}</text>
        </view>
        <view class='eachInforDe' wx:if='{{detail.remark}}'>
          <label class='inforLabel'>备注</label>
          <text class='inforTest'>{{detail.remark}}</text>
        </view>
        <view class='eachInforDe' bindtap='getCodeImg' wx:if='{{detail.status>3&&detail.comment==0&&myClaims}}'>
          <label class='inforLabel'>评价</label>
          <text class='inforTest' style='color:#3891F8'>二维码</text>
        </view>
        <view class='eachInforDe' wx:if='{{detail.status>3&&detail.comment==1}}'>
          <label class='inforLabel'>评价</label>
          <text class='inforTest'>已评价</text>
        </view>
        <view class='eachInforDe' wx:if='{{detail.is_push==1&&detail.task_id!=userId&&hasPush}}'>
          <label class='inforLabel'>推修状态</label>
          <text class='inforTest'>已推修</text>
        </view>
      </view>
    </view>
    <view class='headerDe' wx:if='{{third.length>0}}'>
      <view class='titDe'>三者信息</view>
    </view>
    <view class='third_content' wx:if='{{third.length>0}}'>
      <view class='third_cell' wx:for='{{third}}' wx:for-item='item'>
        <view class='third_tit'>三者信息{{index+1}}</view>
        <view class='eachInforDe'>
          <label class='inforLabel'>联系电话</label>
          <text class='inforTest'>{{item.phone}}</text>
        </view>
        <view class='eachInforDe'>
          <label class='inforLabel'>车牌</label>
          <text class='inforTest'>{{item.car_no}}</text>
        </view>
      </view>
    </view>
    <view wx:if='{{detail.status>2}}'>
      <view class='headerDe'>
        <view class='titDe'>业务时效</view>
      </view>
      <view class="zan-panel" bindtouchstart='enablePage' wx:if='{{detail.status>2}}'>
        <view class="zan-cell zan-cell">
          <view class="zan-cell__bd stepContent">
            <template is="zan-steps1" data="{{ type: 'vertical', hasDesc: true, steps1 }}"></template>
          </view>
        </view>
      </view>
    </view>

    <view wx:if='{{pushSetviceInfor}}'>
      <view class='headerDe' wx:if='{{detail.type==1}}'>
        <view class='titDe'>推修结果</view>
      </view>
      <view class='sectionDe'>
        <view class='eachInforDe'>
          <label class='inforLabel'>推修服务商</label>
          <text class='inforTest'>{{pushSetviceInfor.short_name?pushSetviceInfor.short_name:pushSetviceInfor.name}}</text>
        </view>
        <view class='eachInforDe'>
          <label class='inforLabel'>推修状态</label>
          <text class='inforTest' wx:if='{{pushDetail.result==0}}'>未反馈</text>
          <text class='inforTest' wx:if='{{pushDetail.result==1}}'>成功</text>
          <text class='inforTest' wx:if='{{pushDetail.result==2}}'>失败</text>
          <text class='inforTest' wx:if='{{pushDetail.result==3}}'>不确定</text>
        </view>
      </view>
    </view>

    <view wx:if='{{steps1.length!=0&&detail.type==1}}'>
      <view class='headerDe'>
        <view class='titDe' style='display:inline-block'>推修进度</view>
        <!-- <view class='titDe addPro' bindtap='toAddProgress' wx:if='{{pushDetail.status!=6&&pushDetail.result==1}}'>添加推修进度</view>
        <view class='titDe addPro' bindtap='finishPush' wx:if='{{pushDetail.status!=6&&pushDetail.result==1}}'>完成</view> -->
      </view>
      <view class="zan-panel">
        <view class="zan-cell zan-cell">
          <view class="zan-cell__bd">
            <template is="zan-steps" data="{{ type: 'vertical', hasDesc: true, steps }}"></template>
          </view>
        </view>
      </view>
    </view>


    <view class='sectionDe'>
      <view class='eachInforDe' wx:if='{{ckzpp1.length!=0}}'>
        <view class='detailTit'>人车合一</view>
        <view>
          <view>
            <view class='imgOuter' wx:for='{{ckzpp1}}' wx:item='item'>
              <image bindtap='openImg1' class='detailImg' id='{{item.imgId}}' src='{{hostName}}uploads/work/{{item.path}}'></image>
            </view>
          </view>
        </view>
      </view>

      <view class='eachInforDe' wx:if='{{ckzpp2.length!=0}}'>
        <view class='detailTit'>车架号</view>
        <view wx:if='{{steps[0].outer.length!=0}}'>

          <view class='imgOuter' wx:for='{{ckzpp2}}'>
            <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
          </view>
        </view>
      </view>
      <view class='eachInforDe' wx:if='{{ckzpp3.length!=0}}'>
        <view class='detailTit'>环境照片：事故现场大环境照片，以确认事故成因。</view>
        <view class='imgOuter' wx:for='{{ckzpp3}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>

      </view>
      <view class='eachInforDe' wx:if='{{ckzpp4.length!=0}}'>
        <view class='detailTit'>验车照片：整车斜45°角四个角拍摄。</view>
        <view class='imgOuter' wx:for='{{ckzpp4}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>
      <view class='eachInforDe' wx:if='{{ckzpp5.length!=0}}'>
        <view class='detailTit'>车损照片：本次事故造成损伤部位照片。</view>

        <view class='imgOuter' wx:for='{{ckzpp5}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>
      <view class='eachInforDe' wx:if='{{ckzpp6.length!=0}}'>
        <view class='detailTit'>旧伤确认</view>
        <view class='imgOuter' wx:for='{{ckzpp6}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>

      <view class='eachInforDe' wx:if='{{gydzp1.length!=0}}'>
        <view class='detailTit'>事故证明</view>
        <view class='imgOuter' wx:for='{{gydzp1}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>
      <view class='eachInforDe' wx:if='{{gydzp2.length!=0}}'>
        <view class='detailTit'>索赔申请书</view>
        <view class='imgOuter' wx:for='{{gydzp2}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>
      <view class='eachInforDe' wx:if='{{gydzp3.length!=0}}'>
        <view class='detailTit'>行驶证</view>
        <view class='imgOuter' wx:for='{{gydzp3}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>
      <view class='eachInforDe' wx:if='{{gydzp4.length!=0}}'>
        <view class='detailTit'>驾驶证</view>
        <view class='imgOuter' wx:for='{{gydzp4}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>
      <view class='eachInforDe' wx:if='{{gydzp5.length!=0}}'>
        <view class='detailTit'>查勘报告</view>
        <view class='imgOuter' wx:for='{{gydzp5}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>
      <view class='eachInforDe' wx:if='{{gydzp6.length!=0}}'>
        <view class='detailTit'>个案签报</view>
        <view class='imgOuter' wx:for='{{gydzp6}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>
      <view class='eachInforDe' wx:if='{{gydzp7.length!=0}}'>
        <view class='detailTit'>拒赔材料</view>
        <view class='imgOuter' wx:for='{{gydzp7}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>
      <view class='eachInforDe' wx:if='{{gydzp8.length!=0}}'>
        <view class='detailTit'>从民资格证</view>
        <view class='imgOuter' wx:for='{{gydzp8}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>
      <view class='eachInforDe' wx:if='{{gydzp9.length!=0}}'>
        <view class='detailTit'>法院判决书</view>
        <view class='imgOuter' wx:for='{{gydzp9}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>
      <view class='eachInforDe' wx:if='{{gydzp10.length!=0}}'>
        <view class='detailTit'>调查单证</view>
        <view class='imgOuter' wx:for='{{gydzp10}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>
      <view class='eachInforDe' wx:if='{{zfdzp1.length!=0}}'>
        <view class='detailTit'>收款方账户信息</view>
        <view class='imgOuter' wx:for='{{zfdzp1}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>
      <view class='eachInforDe' wx:if='{{zfdzp2.length!=0}}'>
        <view class='detailTit'>收款方身份证明</view>
        <view class='imgOuter' wx:for='{{zfdzp2}}'>
          <image bindtap='openImg1' id='{{item.imgId}}' class='detailImg' src='{{hostName}}uploads/work/{{item.path}}'></image>
        </view>
      </view>


    </view>
  </view>
  <!--再次更新-->
  <view wx:if='{{addImage}}'>
    <view class='headerDe' wx:if='{{detail.status>3}}'>
      <view class='titDe'>查勘明细</view>
    </view>
    <view class='imageCell' wx:if='{{detail.status>3}}' style='height:{{navHeight}}'>
      <view class='nav' bindtouchmove='stopPage' style='height:{{navHeight}}'>
        <view wx:for='{{listArr}}' wx:for-item="itemName" class='{{itemName.class}}' data-name='{{itemName.dataName}}' bindtap='changeView'>
          <text>{{itemName.name}}</text>
        </view>
      </view>
      <view class='imageContent' style='height:{{navHeight}}'>

        <scroll-view class='imageBack' scroll-y style='height:{{navHeight}}' bindscrolltoupper="upper" bindscrolltolower="lower" bindscroll="scroll" scroll-into-view="{{toView}}" scroll-top="{{scrollTop}}">
          <view id='ckzp1'>
            <view class='tip'>
              <view>
                <text class='picTit'>人车合一</text>
                <view class='addTit' bindtap='toselectTake' id='0' wx:if='{{myClaims}}'>
                  <text>添加</text>
                  <image src='../../../img/rAngle.png' class='rAngle'></image>
                </view>
              </view>
            </view>
            <view class='allCell' data-modul='ckzp1'>

              <view class='imgCell' wx:for='{{ckzpp1}}' wx:item='item'>
                <image bindtap='openImg1' class='plusPic' id='{{item.imgId}}' src='{{hostName}}uploads/work/{{item.path}}'></image>
              </view>

              <view class='imgCell' wx:if='{{ckzpp1.length==0}}' wx:if='{{myClaims}}'>
                <view class='imgborder'>
                  <image class='plusPic' bindtap='toselectTake' src='../../../img/plus.png' id='0'></image>
                </view>
              </view>
            </view>
          </view>
          <view id='ckzp2'>
            <view class='tip'>
              <view>
                <text class='picTit'>车架号</text>
                <view class='addTit' bindtap='toselectTake' id='1' wx:if='{{myClaims}}'>
                  <text>添加</text>
                  <image src='../../../img/rAngle.png' class='rAngle'></image>
                </view>
              </view>
            </view>
            <view class='allCell'>

              <view class='imgCell' wx:for='{{ckzpp2}}' wx:item='item'>
                <image bindtap='openImg1' class='plusPic' id='{{item.imgId}}' src='{{hostName}}uploads/work/{{item.path}}'></image>
              </view>

              <view class='imgCell' wx:if='{{ckzpp2.length==0}}' wx:if='{{myClaims}}'>
                <view class='imgborder'>
                  <image class='plusPic' id='1' bindtap='toselectTake' src='../../../img/plus.png'></image>
                </view>
              </view>
            </view>
          </view>
          <view id='ckzp3'>
            <view class='tip'>
              <view>
                <text class='picTit'>环境照片</text>
                <view class='addTit' bindtap='toselectTake' id='2' wx:if='{{myClaims}}'>
                  <text>添加</text>
                  <image src='../../../img/rAngle.png' class='rAngle'></image>
                </view>
              </view>
            </view>
            <view class='allCell'>

              <view class='imgCell' wx:for='{{ckzpp3}}' wx:item='item'>
                <image bindtap='openImg1' class='plusPic' id='{{item.imgId}}' src='{{hostName}}uploads/work/{{item.path}}'></image>
              </view>
              <view class='imgCell' wx:if='{{ckzpp3.length==0}}' wx:if='{{myClaims}}'>
                <view class='imgborder'>
                  <image id='2' bindtap='toselectTake' class='plusPic' src='../../../img/plus.png'></image>
                </view>
              </view>
            </view>
          </view>
          <view id='ckzp4'>
            <view class='tip'>
              <view>
                <text class='picTit'>验车照片</text>
                <view class='addTit' bindtap='toselectTake' id='3' wx:if='{{myClaims}}'>
                  <text>添加</text>
                  <image src='../../../img/rAngle.png' class='rAngle'></image>
                </view>
              </view>
            </view>
            <view class='allCell'>

              <view class='imgCell' wx:for='{{ckzpp4}}' wx:item='item'>
                <image bindtap='openImg1' class='plusPic' id='{{item.imgId}}' src='{{hostName}}uploads/work/{{item.path}}'></image>
              </view>
              <view class='imgCell' wx:if='{{ckzpp4.length==0}}' wx:if='{{myClaims}}'>
                <view class='imgborder'>
                  <image id='3' bindtap='toselectTake' class='plusPic' src='../../../img/plus.png'></image>
                </view>
              </view>
            </view>
          </view>
          <view id='ckzp5'>
            <view class='tip'>
              <view>
                <text class='picTit'>车损照片</text>
                <view class='addTit' bindtap='toselectTake' id='4' wx:if='{{myClaims}}'>
                  <text>添加</text>
                  <image src='../../../img/rAngle.png' class='rAngle'></image>
                </view>
              </view>
            </view>
            <view class='allCell'>
              <view class='imgCell' wx:for='{{ckzpp5}}' wx:item='item'>
                <image bindtap='openImg1' class='plusPic' id='{{item.imgId}}' src='{{hostName}}uploads/work/{{item.path}}'></image>
              </view>
              <view class='imgCell' wx:if='{{ckzpp5.length==0}}' wx:if='{{myClaims}}'>
                <view class='imgborder'>
                  <image id='4' bindtap='toselectTake' class='plusPic' src='../../../img/plus.png'></image>
                </view>
              </view>
            </view>
          </view>
          <view id='ckzp6'>
            <view class='tip'>
              <view>
                <text class='picTit'>旧伤照片</text>
