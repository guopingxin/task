<view class='modalInforDe' wx:if='{{iffeedback}}'>
  <view class='modalInforContentDe' style='width:280px;background:#fffl;padding-bottom:30px;border-radius:0px'>
    <view style='text-align:center'>
      <view class='result'>
        <view class='result_tit'>推修反馈:</view>
        <button class='{{feedback==1?"active":""}}' style='width:33%' bindtap='feedback_status' id='1'>推修成功</button>
        <button class='{{feedback==2?"active":""}}' bindtap='feedback_status' id='2'>推修失败</button>
        <!-- <button class='{{feedback==3?"active":""}}' bindtap='feedback_status' id='3'>不确定</button> -->
      </view>
    </view>
    <form bindsubmit='feedback' wx:if='{{detail.type==1&&pushDetail.result==0&&detail.is_push==1}}'>
      <view class='result_con'>
        <input type='text' placeholder='提交进度说明信息' value='{{feedback_con}}' name='con' />
        <button form-type='submit' hover-class='none' class='result_sub' wx:if='{{!ok_btn}}'>提交</button>
        <button form-type='submit' hover-class='none' class='result_sub' wx:if='{{ok_btn}}' style='background:#ccc;'>提交</button>
        <button form-type='submit' hover-class='none' style='color:#aaa' bindtap='cancel1' class='result_sub'>取消</button>
      </view>
      <view style='background:#fff;text-align:center;height:20px;'>
      </view>
    </form>
  </view>
</view>
