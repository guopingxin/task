
navigationBar{
  text-align: left;
}
.container{
  width:100%;
  height: 100vh;
  background: rgb(249, 249, 249);
}

.content{
  background: #fff;
  text-align: center;
}
.eachInfor{
  text-align: left;
  width:94%;
  display: inline-block;
  padding: 0px 10px;
  font-size: 14px;
  border-bottom: 1px solid #eee;
  line-height: 46px;
  color:#333333;
}

.eachInfor1{
  text-align: left;
  width:94%;
  display: inline-block;
  padding: 0px 10px;
  font-size: 14px;
  line-height: 46px;
  color:#333333;
}
.damage{

  width: 70%;
  margin-left: 30%;
}


.selectyear{
  border-bottom: 1px solid #ccc;
  width: 94%;
  margin-left: 3%;
}

.selectyear text{
  font-weight:600
}

.selectyear-text{
  color: #3891F8;
  margin-left: 4%;
}

.selectyear-text1{
  color: #3891F8;
  margin-left: 4%;
  font-size: 12px;
}

.dateform{
  width: 70%;
  border-radius: 5px;
  position: fixed;
  top:36%;
  left: 15%;
  background: white;
  opacity: 1;
  z-index: 1000;
}

.year-m-d{
  border-bottom: 1px dotted #5AA3F7;
  text-align: center;
  color: #3891F8;
}

.year{
  width: 33.33%;
  display: inline-block;
  
}

.year1{

  width: 20%;
  display: inline-block;
}

.year2{
  width: 50%;
  display: inline-block;
}

.timecancel{
  width: 50%;
  color: white;
  text-align: center;
  background: rgb(196, 195, 195);
  display: inline-block;
  border-bottom-left-radius: 5px;
}

.timesure{
  width: 50%;
  color: white;
  text-align: center;
  background: #3891F8;
  display: inline-block;
  border-bottom-right-radius: 5px;
}

.date-indicator{

  border-bottom: 1px solid #5AA3F7;
  border-top: 1px solid #5AA3F7;
  text-align: center; 
  
}

.probedate{
  width: 70%;
  display: inline-block;
}

.probedate1{
  width: 60%;
  display: inline-block;
}

.location{
  width: 20px;
  height: 20px;
  position: relative;
  left: 4%;
  top:4px;
}

label{
  display: inline-block;
  width:30%;
}
input{
  display: inline-block;
  width:180px;
  vertical-align: middle;
}
radio{
  display: inline-block;
  width:30%;
}
.btn{
  width:94%;
  text-align: center;
  background: #3891F8;
  color:#fff;
  height: 40px;
  line-height: 40px;
  font-size: 18px;
  margin-top:10px;
  border-radius: 0px; 
}
radio-group{
  display: inline-block;
  width:70%;
}

.componyPos{
  position: relative;
}
.redirect{
  float:right;
  color:#5AA3F7;
  text-decoration: underline
}
.modal,.carHostModal{
  position: fixed;
  width:100%;
  height: 100vh;
  top:0px;
  left:0px;
  background: rgba(150,150,150,0.6)
}
.modalContent{
  position: fixed;
  width:100%;
  bottom: 0px;
  background: #fff;
  padding: 10px 0px;
  color:#6F6F6F;
}
.modaleach{
  line-height: 40px;
  font-size: 14px;
  padding-left: 20px
}
.forbide{
  width:18px;
  height: 18px;
  vertical-align: middle;
}
.carHostcontent{
  width:70%;
  background: #fff;
  margin:0px auto;
  margin-top:50%;
  border-radius:10px;
}
.confirm{
  padding: 20px;
  color:#080808;
  border:1px solid #ccc;
  text-align: center;
}
.cancel{
  display:inline-block;
  width:48%;
  text-align:center;
  color:#080808;
  
}
.yes{
  color:#218918;
  padding:10px 0px;
  
}
.no{
  border-left:1px solid #ccc;
   padding:10px 0px;
}
.company{
  text-align: center;
  padding-top: 10px;
  position:absolute;
  top:30px;
  line-height: 30px;
  background: #fff;
  z-index:2;
  left:26%;
  width:30%;
  border:1px solid #ddd;
  border-top:none;
  border-radius:3px;
}
.down{
  position: absolute;
  top:16px;
  margin-left:10px;
  width:12px;
  height: 12px;
}
.redInfor{
  font-size: 12px;
}

picker{
  display: inline-block;
  width:70%;
}
radio{
  display: inline-block;
  width:50%;
}
.addThird{
  font-size: 14px;
  display: inline-block;
  color:#3891F8;
  width:200px;
  text-decoration: underline;
  margin-right: 15px;
  padding: 14px 0px 6px 0px;
}
.third_tit{
  font-size: 14px;
  display: inline-block;
  width:94%;
  color:#5AA3F7;
  text-align: left;
}
.third_tit image{
  width:20px;
  height: 20px;
  float:right;
  vertical-align: middle;
}
.third_cell{
  background: #f7f7f7;
  text-align: center;
  padding-top: 10px;
}
