/* pages/index/addPeople/addPeople.wxss */


navigationBar{
  text-align: left;
}
.container{
  width:100%;
  height: 100vh;
  background: rgb(249, 249, 249);
}
.content{
  padding: 0px 15px;
  background: #fff;
}
.eachInfor{
  font-size: 14px;
  line-height: 36px;
  border-bottom: 1px solid #eee;
  line-height: 46px;
  color:#333333;
}
label{
  display: inline-block;
  width:22%;
}
input{
  display: inline-block;
  width:180px;
  vertical-align: middle;
}
radio{
  display: inline-block;
  width:30%;
}
.btn{
  width:100%;
  text-align: center;
  background: #3891F8;
  color:#fff;
  height: 40px;
  line-height: 40px;
  border-radius: 10px;
  font-size: 18px;
  margin-top:10px; 
}
radio-group{
  display: inline-block;
  width:70%;
}

.tochooseBtn{
  display:inline-block;
  width:80px;
  height:36px;
  background:#3891F8;
  color:#fff;
  line-height:36px;
  vertical-align:middle;
  font-size:14px;
}
.analyze{
  color:#44CB5A;
  font-size:12px;
  text-align:right;
  text-decoration:underline;
  line-height:26px;
}